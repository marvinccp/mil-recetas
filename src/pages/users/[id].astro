---
export async function getStaticPaths() {

  const data = await (
    await fetch("https://jsonplaceholder.typicode.com/users")
  ).json();
  
 
  return data.map((user) => {

    
    return {
      params: { id: user.id },
      props: { user },
    };
  });
}

const { id } = Astro.params;
const { user } = Astro.props;
---

<h3>id: {id}</h3>
<h1>{user.name}</h1>

